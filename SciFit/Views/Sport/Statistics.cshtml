@model SciFit.Models.StatisticsModel
<div class="container">
    <div class="col-lg-12 col-sm-12">
        <div>Start date: @Model.StartDate</div>
        <div>Done count: @Model.DoneCount</div>
        <div>Current level: @Model.CurrentLvl</div>
        <div class="row" style="display: flex; justify-content: center">
            <div class="col-md-12">
                @Html.ActionLink("Cancel", "Plan", "Sport", null,
                            new { @class = "btn btn-danger", style = "margin: 0 auto;" })
            </div>
        </div>
    </div>
</div>